citHeader("To cite this package, please use:")

citEntry(
  entry    = "Article",
  title    = "articleTitle",
  author   = "author1, author1, author3",
  journal  = "journalName",
  year     = "publicationYear",
  volume   = "volumeNumber",
  number   = NULL,
  pages    = NULL,
  url      = "linkToPublication",
  textVersion = paste("author1, author1, author3 (publicationYear) articleTitle, *journalName*; volumeNumber, [linkToPublication](linkToPublication)"
  )
)
